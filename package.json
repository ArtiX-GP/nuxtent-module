    "gray-matter": "^4.0.1",
    "js-yaml": "^3.12.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.10",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "nuxt": "^2.3.4",
