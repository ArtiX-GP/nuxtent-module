{
  "presets": [
    ["es2015", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    "transform-async-to-generator"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "stage-2"
      ],
      "plugins": [
        "istanbul",
        [
          "transform-object-rest-spread",
          {
            "useBuiltIns": true
          }
        ]
      ],
      "sourceMaps": "inline"
    }
  },
  "comments": false
}
